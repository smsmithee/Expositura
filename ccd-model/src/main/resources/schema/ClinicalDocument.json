{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://expositura.health/2020-12/schema/ClinicalDocument",
  "title": "CCD Schema",
  "type": "object",
  "properties": {
    "nullFlavor": {
      "type": "string"
    },
    "classCode": {
      "type": "string"
    },
    "moodCode": {
      "type": "string"
    },
    "realmCode": {
      "type": "array",
      "items": {
        "$ref": "CS.json"
      }
    },
    "typeId": {
      "$ref": "II.json"
    },
    "templateId": {
      "type": "array",
      "items": {
        "$ref": "II.json"
      }
    },
    "id": {
      "$ref": "II.json"
    },
    "sdtcCategory": {
      "type": "array",
      "items": {
        "$ref": "CD.json"
      }
    },
    "code": {
      "$ref": "CE.json"
    },
    "title": {
      "$ref": "ST.json"
    },
    "sdtcStatusCode": {
      "$ref": "CS.json"
    },
    "effectiveTime": {
      "$ref": "TS.json"
    },
    "confidentialityCode": {
      "$ref": "CE.json"
    },
    "languageCode": {
      "$ref": "CS.json"
    },
    "settId": {
      "$ref": "II.json"
    },
    "versionNumber": {
      "$ref": "INTT.json"
    },
    "copyTime": {
      "$ref": "TS.json"
    },
    "recordTarget": {
      "type": "array",
      "items": {
        "$ref": "RecordTarget.json"
      }
    },
    "author": {
      "type": "array",
      "items": {
        "$ref": "Author.json"
      }
    }
  }
}
